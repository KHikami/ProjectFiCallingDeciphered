<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item android:left="1dp" android:right="1dp" android:bottom="-1dp">
        <shape>
            <corners android:radius="4dp" />
            <solid android:color="@ba/jb" />
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape>
                    <corners android:radius="4dp" />
                    <solid android:color="0x7f0e002a" />
                    <stroke android:width="1px" android:color="@ba/jb" />
                </shape>
            </item>
            <item>
                <shape>
                    <corners android:radius="4dp" />
                    <solid android:color="0x7f0e002b" />
                    <stroke android:width="1px" android:color="@ba/jb" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>
